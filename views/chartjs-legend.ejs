<div id="<%=id%>-main-container">
  <div class="graph-title">
    <button type="button" class="btn btn-danger" id="left<%=id%>">
      <i class="fas fa-arrow-left"></i>
    </button>
    <p><%=title%></p>
    <button type="button" class="btn btn-danger" id="right<%=id%>">
      <i class="fas fa-arrow-right"></i>
    </button>
  </div>

  <div class="graph-content">
    <div class="col-sm graph-container" id="<%=id%>container">
      <div id="<%=id%>sub-container">
        <canvas id="<%=id%>" height="350"></canvas>
      </div>
    </div>

    <div class="main-complements-chart">
      <div class="legend-container">
        <p class="little-box bg-blue"></p>
        <p id="mean<%=id%>">Mean:</p>

        <p class="little-box bg-orange"></p>
        <p id="Sgma<%=id%>">Sgma:</p>

        <p class="little-box bg-green"></p>
        <p id="VcPk<%=id%>">VcPk:</p>

        <p class="little-box bg-red"></p>
        <p id="LcPk<%=id%>">LcPk:</p>
      </div>

      <div class="mt-4 col w-50 zoom-buttons">
        <div class="mx-auto">
          <i id="<%=id%>-plus" class="fas fa-search-plus m-auto material-icons hov-blue"></i>
        </div>
        <div class="mx-auto">
          <p id="<%=id%>-percentage" class="mx-auto my-3"></p>
        </div>
        <div class="mx-auto">
          <i id="<%=id%>-minus" class="fas fa-search-minus m-auto material-icons hov-blue"></i>
        </div>
        
      </div>
      
    </div>
  </div>
</div>
